package test_super;

public class Dog extends Animal{
	String color = "Black";
	void print_color(){
		System.out.println("The color of the dog: " + color);
		System.out.println("The color of the animal: " + super.color);
	}
	
	public Dog(){
		super();
	}
	
	void eat(){
		System.out.println("The dog is eating");
	}
	
	void test_eat(){
		super.eat();
	}

}
