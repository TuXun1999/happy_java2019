package test_inheritance;

public class test_inheritance {
	public static void main(String[] args){
		CalculationMethods ex = new CalculationMethods();
		System.out.println("Remains " + ex.remains(5,2));
		System.out.println("Addition: " + ex.addition(5,2));
		System.out.println("Substraction: " + ex.substraction(4,2));
		System.out.println("Multi: " + ex.multiplication(4,3));
		System.out.println("Div: " + ex.division(4, 2));
	}

}
