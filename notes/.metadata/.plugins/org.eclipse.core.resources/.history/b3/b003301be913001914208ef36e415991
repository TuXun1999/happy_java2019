package condition;

public class boolen_test {
	public static void main(String[] args){
		//Part I: boolen macro?
		int a = 12;
		int b = 3;
		
		String sup = 
		(a < b) ? "smaller": (a > b) ? "bigger" : "equal";
		/*
		 * if a < b, sup = "smaller"
		 * else if a > b, sup = "bigger"
		 * else, sup = "equal"
		 */
		System.out.println(sup);
		
		double a1 = 3.5;
		double b1 = 3.5;
		String sup1 = 
		(a1 == b1) ? "equal" : (a1 > b1) ? "bigger" : "smaller";
		System.out.println(sup1);
	}
}
