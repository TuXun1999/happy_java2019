	public static void main(String[] args){
		int a = 8;
		int b = 10;
		
		Lambda test = (double c, double d) -> {
			if (d == 0) return 0;
			else
				return c/d;
		};
		
		System.out.println(test.foo(a, b));
	}
	interface Lambda{
		public double foo(double a, double b);
	}
}
