package loop;

public class test_loop {
	public static void main(String[] args){
		//Part I: print out a list 
		int num = 10;
		while (num<= 50){
			System.out.println("Number: " + num);
			num ++;
		}
		
		//Part II: power 
		int N = Integer.parseInt(args[0]);
		System.out.println("Power: " + power(N));
	}
	
	static int power(int N){
		int result = 1;
		while (N > 0){
			result *= 2;
			N --;
		}
		return result;
	}
}
