package test_inheritance;

public class CalculationMethods extends Calculation{
	/*
	 * subclass
	 */
	public int addition(int a, int b){
		return (a + b);
	}
	
	public int substraction(int a, int b){
		return (a - b);
	}
	
	public int multiplication(int a, int b){
		return (a*b);
	}
	
	public int division(int a, int b){
		return (a/b);
	}
	/*
	 * override the methods in the super class
	 * (repeat the methods in subclass with same name but different contains)
	 */
	public double remains(double a, double b){
		return (a%b)*2;
	}
}
