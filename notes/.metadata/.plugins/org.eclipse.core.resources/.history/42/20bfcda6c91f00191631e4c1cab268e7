package sorting_and_searching;

public class insertion {
	public void sort(int[] a){
		for(int i = 0; i < a.length-1; i++){
			if(a[i] > a[i+1]){
				for(int j = i; j >= 0; j--){
					if(a[j] > a[j+1]){
						int tmp = a[j+1];
						a[j+1] = a[j];
						a[j] = tmp;
					}
					else{
						break;
					}
				}
			}
		}
	}

}
