package Datatype;

public class TestCom {
	//< --name-- > means introducing the generic data type, with a specific name
	public static <T extends Comparable<T>> T greatest(T x, T y, T z){
		//T means a new generic type. It is sensible within the block
		T tmp;
		if (x.compareTo(y) > 0){
			tmp = x;
		}
		else
			tmp = y;
		
		if (tmp.compareTo(z) > 0){ 
			//In generic data type, some methods, types are not the same with the original ones
			return tmp;
		}
		else
			return z;
			
	}
	

}
